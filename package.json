{
  "name": "storybook-csf",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^7.4.0-alpha.1",
    "@storybook/addon-interactions": "^7.4.0-alpha.1",
    "@storybook/addon-links": "^7.4.0-alpha.1",
    "@storybook/addon-storyshots": "^7.3.2",
    "@storybook/blocks": "^7.4.0-alpha.1",
    "@storybook/svelte": "^7.4.0-alpha.1",
    "@storybook/sveltekit": "^7.4.0-alpha.1",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^1.0.0-next.90",
    "@sveltejs/kit": "^1.0.0-next.587",
    "@sveltejs/package": "^1.0.0-next.6",
    "@testing-library/jest-dom": "^5.14.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/jest": "^27.0.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "babel-jest": "^27.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^27.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.0-alpha.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-jester": "^2.0.1",
    "ts-jest": "^27.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module"
}
